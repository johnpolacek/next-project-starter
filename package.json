{
  "name": "project-starter",
  "description": "Get your next React project up and running quickly with Next.js, Theme UI, MDX and more.",
  "version": "0.1.1",
  "scripts": {
    "cypress": "cypress open",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format-watch": "npm run format && onchange \"**/*.{js,jsx,json,md}\" -- prettier --write {{changed}}",
    "test": "cypress run",
    "deploy": "npm run test && git push origin master && now --prod"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@mdx-js/loader": "^1.6.16",
    "@next/mdx": "^9.5.3",
    "cypress": "^5.1.0",
    "next": "^9.5.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "onchange": "^7.0.2",
    "prettier": "^2.1.1"
  },
  "repository": "https://github.com/johnpolacek/project-starter",
  "license": "MIT-0"
}
