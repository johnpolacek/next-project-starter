{
  "name": "project-starter",
  "description": "Get your next React project up and running quickly with Next.js, Theme UI, MDX and more.",
  "version": "0.1.1",
  "scripts": {
    "cypress": "cypress open",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format-watch": "npm run format && onchange \"**/*.{js,jsx,json,md}\" -- prettier --write {{changed}}",
    "test": "cypress run",
    "deploy": "npm run test && git push origin master && now --prod"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@mdx-js/loader": "^1.6.21",
    "@next/mdx": "^10.0.3",
    "cypress": "^6.1.0",
    "next": "^10.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "theme-ui": "^0.3.4"
  },
  "devDependencies": {
    "onchange": "^7.1.0",
    "prettier": "^2.2.0"
  },
  "resolutions": {
    "@babel/runtime": "^7.12.5"
  },
  "repository": "https://github.com/johnpolacek/project-starter",
  "license": "MIT-0"
}
